{
  "name": "scarlet-notes-to-markdown",
  "version": "1.0.0",
  "description": "Converts a Scarlet Notes backup to flat Markdown files",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "yarn eslint && yarn prettier --check",
    "lint:fix": "yarn eslint --fix && yarn prettier --write",
    "eslint": "eslint '**/*.{js,jsx,ts,tsx,json}'",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx,json,css,md}'",
    "test": "jest --passWithNoTests",
    "test:smoke": "echo help me write one"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tlvince/scarlet-notes-to-markdown.git"
  },
  "keywords": [
    "template"
  ],
  "author": "Tom Vincent <git@tlvince.com> (https://tlvince.com)",
  "bugs": {
    "url": "https://github.com/tlvince/scarlet-notes-to-markdown/issues"
  },
  "homepage": "https://github.com/tlvince/scarlet-notes-to-markdown#readme",
  "files": [
    "dist/"
  ],
  "engines": {
    "npm": ">=6.12",
    "node": ">=10",
    "yarn": ">=1.19"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/plugin-transform-regenerator": "7.14.5",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-typescript": "7.14.5",
    "@babel/runtime": "7.14.8",
    "@commitlint/config-conventional": "13.1.0",
    "@types/jest": "26.0.24",
    "@types/node": "16.4.5",
    "babel-jest": "27.0.6",
    "commitlint": "13.1.0",
    "eslint": "7.31.0",
    "eslint-config-tlvince": "2.1.40",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lint-staged": "11.1.1",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  }
}
